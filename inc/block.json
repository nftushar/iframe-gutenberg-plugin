{
  "apiVersion": "1",
  "name": "ifm/iframe",
  "title": "iframe",
  "description": "I Frame-7 Gutenberg plugin: Streamline Gutenberg editing with easy-to-use, responsive iframe embedding for videos, maps, and more.",
  "category": "media",
  "keywords": ["iframe-7", "iframe", "Gutenberg"],
  "textdomain": "iframe-7",

  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "cId": {
      "type": "string",
      "default": ""
    },
    "src": {
      "type": "string",
      "default": "https://bplugins.com/"
    },
    "title": {
      "type": "string",
      "default": "Iframe title"
    },
    "width": {
      "type": "string",
      "default": "100%"
    },
    "height": {
      "type": "string",
      "default": "673px"
    },
    "border": {
      "type": "object",
      "default": {
        "radius": "20px",
        "width": "16px",
        "color": "rgba(255, 179, 179, 0.41)"
      }
    },
    "shadow": {
      "type": "array",
      "default": [
        {
          "hOffset": "0px",
          "vOffset": "4px",
          "blur": "8px",
          "spreed": "0px",
          "color": "#3FB9C2A3"
        }
      ]
    },
    "loading": {
      "type": "string",
      "default": "auto"
    },
    "isFullScreen": {
      "type": "boolean",
      "default": true
    }
  },
  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true,
      "columns": {
        "desktop": 1,
        "tablet": 1,
        "mobile": 1
      }
    }
  },
  "editorScript": "file:../dist/editor.js"
}
